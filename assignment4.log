24.Write a function to find the intersection of two arrays (common elements between the two arrays).
ex:- arr1 = [2,4,1,7,4,2] , arr2 = [5,1,4,7,3,1];
output = [1,4,7]


function intersection(x, y) {
    var n1 = x.sort();
    var n2 = y.sort();
    var i = 0;
    var j = 0;
    var k =0;
    var ans = [];
    while(i < n1.length && j < n2.length){
        if(n1[i] == n2[j]){
            ans[k] = n1[i];
            i++;
            j++;
            k++;
        }
        else if(n1[i]<n2[j]){
            i++;
        }
        else{
            j++;
        }
    }
    return ans;
};



